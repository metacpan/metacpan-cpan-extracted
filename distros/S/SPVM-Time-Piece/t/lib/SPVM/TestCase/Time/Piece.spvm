  static method localtime : int () {
    
    {
      my $tp = Time::Piece->localtime(0);
      
      warn "[Test Output]localtime::" . $tp->cdate;
      
      unless (Fn->contains($tp->cdate, "1970")) {
        return 0;
      }
    }
    
    {
      my $tp = Time::Piece->localtime;
      
      warn "[Test Output]localtime:no_args:" . $tp->cdate;
      
      unless ($tp->epoch > 0) {
        return 0;
      }
    }
    
    return 1;
  }
  
