# Function: get - using bdb file directly
#
# $Id$

use AC::Dumper;
use AC::Yenta::Direct;
use strict;

my $map  = shift @ARGV;
my $file = shift @ARGV;
my $key  = shift @ARGV;

my $y = AC::Yenta::Direct->new( $map, $file );
my $v = $y->get($key);
print  dumper($v), "\n";

