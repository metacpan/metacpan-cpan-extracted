INPUT

T_TIFF
   if (sv_derived_from($arg, \"Graphics::TIFF::Image\")) {
       $var = INT2PTR (TIFF, SvIV ((SV*)SvRV ($arg)));
   } else {
       croak(\"$var is not of type Graphics::TIFF::Image\");
   }


OUTPUT

T_TIFF
