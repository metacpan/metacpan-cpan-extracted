	doap-changeset:changeset [
		doap-changeset:item [
			a doap-changeset:Change;
			rdfs:label "Use attr=>{extends=>true} or Moose-like +attr to extend an inherited attribute.";
		], [
			a doap-changeset:Change;
			rdfs:label "Marlin now checks for conflicts between methods and init_args used by different attributes, so you can't (for example) have two attributes that have the same-named reader method.";
		], [
			a doap-changeset:Change;
			rdfs:label "An attribute that creates lexical accessors in a parent class won't automatically create matching ones in child classes.";
		], [
			a doap-changeset:Documentation;
			rdfs:label "Minor changes and reformatting of extended examples to make things cleaner.";
		], [
			a doap-changeset:Change;
			rdfs:label "In Perl versions too old to support lexical subs, use namespace::clean to clean away any non-lexical ones we wanted to be lexical.";
		], [
			a doap-changeset:Bugfix;
			rdfs:label "Marlin::Attribute sometimes thought its link back to the package it was defined in was called `marlin` and sometimes `_marlin`; fix it to always be the former.";
		];
	];
