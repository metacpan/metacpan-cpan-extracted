         "rdfs:label" : "actor",
         "schema:domainIncludes" : [
            "schema:Episode",
            "schema:VideoObject",
            "schema:Event",
            "schema:Clip",
            "schema:RadioSeries",
            "schema:CreativeWorkSeason",
            "schema:VideoGameSeries",
            "schema:MovieSeries",
            "schema:Movie",
            "schema:VideoGame",
            "schema:TVSeries"
         ],
         "schema:rangeIncludes" : "schema:Person"
      },
      "schema:actors" : {
         "rdf:type" : "rdf:Property",
         "rdfs:comment" : "An actor, e.g. in tv, radio, movie, video games etc. Actors can be associated with individual items or with a series, episode, clip.",
         "rdfs:label" : "actors",
         "schema:domainIncludes" : [
            "schema:Episode",
            "schema:VideoObject",
            "schema:Clip",
            "schema:RadioSeries",
            "schema:MovieSeries",
            "schema:Movie",
