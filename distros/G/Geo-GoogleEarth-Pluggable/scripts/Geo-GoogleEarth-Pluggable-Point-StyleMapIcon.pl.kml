<?xml version="1.0" encoding="utf-8"?>
<kml xmlns="http://www.opengis.net/kml/2.2" xmlns:kml="http://www.opengis.net/kml/2.2" xmlns:gx="http://www.google.com/kml/ext/2.2" xmlns:atom="http://www.w3.org/2005/Atom">
  <Document>
    <name>StyleMap Example</name>
    <Snippet maxLines="0"/>
    <Style id="Style-perl-0">
      <IconStyle>
        <color>ffc00000</color>
        <Icon>
          <href>http://maps.google.com/mapfiles/kml/shapes/shaded_dot.png</href>
        </Icon>
      </IconStyle>
    </Style>
    <Style id="Style-perl-1">
      <IconStyle>
        <color>ff00c000</color>
        <Icon>
          <href>http://maps.google.com/mapfiles/kml/shapes/shaded_dot.png</href>
        </Icon>
      </IconStyle>
    </Style>
    <StyleMap id="StyleMap-perl-2">
      <Pair>
        <key>normal</key>
        <styleUrl>#Style-perl-0</styleUrl>
      </Pair>
      <Pair>
        <key>highlight</key>
        <styleUrl>#Style-perl-1</styleUrl>
      </Pair>
    </StyleMap>
    <Placemark>
      <name>Style1 Point</name>
      <Snippet maxLines="0"/>
      <styleUrl>#Style-perl-0</styleUrl>
      <Point>
        <coordinates>-77.037579,38.893873,0</coordinates>
      </Point>
    </Placemark>
    <Placemark>
      <name>StyleMap Point</name>
      <Snippet maxLines="0"/>
      <styleUrl>#StyleMap-perl-2</styleUrl>
      <Point>
        <coordinates>-77.036579,38.893873,0</coordinates>
      </Point>
    </Placemark>
    <Placemark>
      <name>Style2 Point</name>
      <Snippet maxLines="0"/>
      <styleUrl>#Style-perl-1</styleUrl>
      <Point>
        <coordinates>-77.035579,38.893873,0</coordinates>
      </Point>
    </Placemark>
  </Document>
</kml>
