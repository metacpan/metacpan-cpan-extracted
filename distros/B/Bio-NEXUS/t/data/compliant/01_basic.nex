#NEXUS

BEGIN TAXA;
      dimensions ntax=4;
      taxlabels A B C D;  
END;

BEGIN CHARACTERS;
      dimensions nchar=5;
      format datatype=protein gap=-;
      charlabels 1 2 3 4 Five;
      matrix
A     MA-LL
B     MA-LE
C     MEATY
D     ME-TE
;
END;

BEGIN TREES;
       tree 'basic bush' = ((A:1,B:1):1,(C:1,D:1):1);
END;
