	lib/DateTime/Astro.pm \
	lib/DateTime/Event/SolarTerm.pm
	$(NOECHO) $(POD2MAN) --section=3 --perm_rw=$(PERM_RW) -u \
	  lib/DateTime/Astro.pm $(INST_MAN3DIR)/DateTime::Astro.$(MAN3EXT) \
	  lib/DateTime/Event/SolarTerm.pm $(INST_MAN3DIR)/DateTime::Event::SolarTerm.$(MAN3EXT) 




