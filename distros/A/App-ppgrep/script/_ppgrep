#sub new {
#    my ($class, %opts) = @_;
#
#    # from FromJSON
#    $opts{"JSON::PP::Boolean"} //= ['one_or_zero'];
#    $opts{"JSON::XS::Boolean"} //= ['one_or_zero']; # this doesn't exist though
#    $opts{"Cpanel::JSON::XS::Boolean"} //= ['one_or_zero']; # this doesn't exist though
