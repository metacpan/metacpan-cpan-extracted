         "schema:domainIncludes" : "schema:Product",
         "schema:rangeIncludes" : "schema:Text"
      },
      "schema:depth" : {
         "dc:source" : "schemadotorgsources:source_GoodRelationsTerms",
         "rdf:type" : "rdf:Property",
