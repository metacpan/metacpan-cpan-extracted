sub pdl2aref {
  my $pdl = shift;
  my @out = ();
  my @in = $pdl->list;
  my $beginning = 1;
  for (@in) {
    next if $beginning && $_ && $_ eq 'BAD';
    $beginning = 0;
    push @out, $_;
  }
  return \@out;
}

