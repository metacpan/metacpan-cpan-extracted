<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    % for ( items_in 'urls' ) {
        <url>
            <loc><%= $_->{url} %></loc>
            <changefreq><%= $_->{freq} %></changefreq>
            <priority><%= $_->{priority} %></priority>
        </url>
    % }
</urlset>