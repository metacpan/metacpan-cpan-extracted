#!/usr/bin/env perl
# PODNAME: pdf2json
# ABSTRACT: Command pdf2json of Alien-pdf2json

$|=1;

use strict;
use warnings;
use File::ShareDir ':ALL';
use Path::Class;

my $abs = file(dist_dir('Alien-pdf2json'),'bin','pdf2json')->cleanup->absolute;

exec($abs, @ARGV) or print STDERR "couldn't exec pdf2json: $!";

__END__

