  package main;

  use Venus::Test;

  my $new = Venus::Test->new('t/Venus_Test.t');

  # bless(..., "Venus::Test")

=back

=over 4

=item new example 3

  package main;

  use Venus::Test;

  my $new = Venus::Test->new(file => 't/Venus_Test.t');

  # bless(..., "Venus::Test")

=back

=over 4

=item B<may raise> L<Venus::Test::Error> C<on.new>

  package main;

  use Venus::Test;

  my $test = Venus::Test->new('t/data/no-name.t');

  # Error! (on.new)

=back

