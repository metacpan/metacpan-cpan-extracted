      "schema:trailer" : {
         "rdf:type" : "rdf:Property",
         "rdfs:comment" : "The trailer of a movie or tv/radio series, season, episode, etc.",
         "rdfs:label" : "trailer",
         "schema:domainIncludes" : [
            "schema:Episode",
            "schema:VideoGame",
            "schema:VideoGameSeries",
            "schema:TVSeries",
            "schema:RadioSeries",
            "schema:MovieSeries",
            "schema:CreativeWorkSeason",
            "schema:Movie"
