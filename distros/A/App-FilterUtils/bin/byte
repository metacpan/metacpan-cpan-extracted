#!/usr/bin/perl -CA

use strict;
use warnings;

use App::FilterUtils::byte;
our $VERSION = '0.001'; # VERSION

App::FilterUtils::byte->run();

exit 0;

__END__

=head1 Command-line utility

byte - Format input bytes with IEC prefixes

