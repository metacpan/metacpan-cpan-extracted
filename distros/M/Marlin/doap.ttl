<http://purl.org/NET/cpan-uri/dist/Marlin/v_0-008000>
	a                    doap:Version;
	dc:identifier        "Marlin-0.008000"^^xsd:string;
	dc:issued            "2025-12-23"^^xsd:date;
	doap-changeset:changeset [
		doap-changeset:item [
			a doap-changeset:Documentation;
			rdfs:label "Omit -strict from examples as that is now the default.";
		], [
			a doap-changeset:Documentation;
			rdfs:label "Document API.";
		], [
			a doap-changeset:Documentation;
			rdfs:label "Better documentation of Marlin::Struct.";
		], [
			a doap-changeset:Change;
			rdfs:label "Marlin can now inherit from Moose and Moo classes and compose Moose and Moo roles.";
		], [
			a doap-changeset:Change;
			rdfs:label "Internally use Clone::clone() instead of Storage::dclone() as it can cope with coderefs.";
		], [
			a doap-changeset:Change;
			rdfs:label "Classes built with Marlin::Struct now provide a default stringification and arrayrefification.";
		], [
			a doap-changeset:Change;
			rdfs:label "Various small improvements to Marlin::Struct.";
		], [
			a doap-changeset:Change;
			rdfs:label "Provide some useful utility API methods.";
		];
	];
