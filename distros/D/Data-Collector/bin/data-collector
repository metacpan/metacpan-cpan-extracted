#!/usr/bin/perl
use strictures 1;
package
    datacollector;
# ABSTRACT: command line interface for Data::Collector via Data::Collector::App

use Data::Collector::App;

my $app = Data::Collector::App->new_with_options;

exit $app->run;

__END__
