#!/usr/bin/perl -w

require 5.002;

use Locale::Codes::Country;

print code2country("BO"),"\n";
print code2country("BO",LOCALE_CODE_ALPHA_2),"\n";
print code2country("bol",LOCALE_CODE_ALPHA_3),"\n";

