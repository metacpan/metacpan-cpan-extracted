#cmakedefine C_INLINE @C_INLINE@
#cmakedefine C_INLINE_IS_INLINE @C_INLINE_IS_INLINE@
#ifndef __cplusplus
#  ifndef C_INLINE
#    define inline
#  else
#    ifndef C_INLINE_IS_INLINE
/* Next line is never executed when inline is "inline" */
#      define inline C_INLINE
#    endif
#  endif
#endif

