			a doap-changeset:Addition;
			rdfs:label "Support for alias.";
		], [
			a doap-changeset:Documentation;
			rdfs:label "Update benchmarks as we are now significantly faster than Mouse::XS.";
		], [
			a doap-changeset:Documentation;
			rdfs:label "Update copyright dates.";
		], [
			a doap-changeset:Addition;
			rdfs:label "Class::XSDelegation.";
		], [
			a doap-changeset:Addition;
			rdfs:label "Undocumented function Class::XSConstructor::install_reader().";
		];
	];
