chronosadmin_p=Mot de passe MySQL
chronosadmin_username=Nom d'usager (max. 16 caractères)
chronosadmin_email=Adresse de courriel
chronosadmin_lang=Langue
chronosadmin_realname=Nom réel de l'usager
chronosadmin_connectok=Connecté à la base de données MySQL
